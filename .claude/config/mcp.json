{
  "servers": {
    "tmux-orchestrator": {
      "command": "tmux-orc",
      "args": [
        "server",
        "start"
      ],
      "env": {
        "TMUX_ORC_MCP_MODE": "claude",
        "PYTHONUNBUFFERED": "1"
      },
      "disabled": false,
      "description": "AI-powered tmux session orchestrator"
    }
  }
}
